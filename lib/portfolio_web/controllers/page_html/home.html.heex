<!-- Certifications Section - Fixed contrast -->
<section class="text-gray-800 dark:text-gray-200 bg-gray-50 dark:bg-gray-900 body-font transition-colors" aria-labelledby="certifications-heading">
  <div class="container px-5 py-10 mx-auto">
    <div class="flex flex-col text-center w-full">
      <h2 id="certifications-heading" class="sm:text-3xl text-2xl font-medium title-font mb-4 text-gray-900 dark:text-white">
        Certifications!
      </h2>
      <p class="lg:w-2/3 mx-auto leading-relaxed text-base text-gray-700 dark:text-gray-300 mb-8">
        Professional certifications and credentials that validate my expertise in cloud platforms, DevOps practices, and platform engineering.
      </p>
    </div>
    <%= if @badges do %>
      <div class="flex flex-wrap justify-center -m-4 text-center" role="list" aria-label="Professional certifications">
        <%= for badge <- @badges["data"] do %>
          <div class="p-4 md:w-1/5 sm:w-1/2 w-full" role="listitem">
            <div class="px-4 py-6 flex items-center justify-center rounded-lg bg-white border border-gray-200 hover:border-gray-300 hover:shadow-lg transition-all duration-200 focus-within:ring-2 focus-within:ring-blue-500 focus-within:border-blue-500">
              <div
                data-iframe-width="250"
                data-iframe-height="250"
                data-share-badge-id={badge["id"]}
                data-share-badge-host="https://www.credly.com"
                role="img"
                aria-label={"Professional certification badge: #{badge["name"] || "Certification"}"}>
              </div>
            </div>
          </div>
        <% end %>
      </div>
      <script type="text/javascript" async src="//cdn.credly.com/assets/utilities/embed.js"></script>
    <% else %>
      <div class="text-center py-12">
        <div class="inline-flex items-center justify-center w-16 h-16 mb-4 rounded-full bg-gray-200" aria-hidden="true">
          <svg fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" class="w-8 h-8 text-gray-600" viewBox="0 0 24 24" role="img" aria-label="Certificate icon">
            <path d="M9 12l2 2 4-4M7.835 4.697a3.42 3.42 0 001.946-.806 3.42 3.42 0 014.438 0 3.42 3.42 0 001.946.806 3.42 3.42 0 013.138 3.138 3.42 3.42 0 00.806 1.946 3.42 3.42 0 010 4.438 3.42 3.42 0 00-.806 1.946 3.42 3.42 0 01-3.138 3.138 3.42 3.42 0 00-1.946.806 3.42 3.42 0 01-4.438 0 3.42 3.42 0 00-1.946-.806 3.42 3.42 0 01-3.138-3.138 3.42 3.42 0 00-.806-1.946 3.42 3.42 0 010-4.438 3.42 3.42 0 00.806-1.946 3.42 3.42 0 013.138-3.138z"></path>
          </svg>
        </div>
        <h3 class="text-lg font-medium text-gray-900 mb-2">Certifications Loading</h3>
        <p class="text-gray-700">Professional certifications are being loaded...</p>
      </div>
    <% end %>
  </div>
</section>

